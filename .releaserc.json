{
  "branches": [
    {
      "name": "master"
    },
    {
      "name": "Automatic_version_update_dependabot"
    },
    {
      "name": "feat-*",
      "prerelease": true
    },
    {
      "name": "fix-*",
      "prerelease": true
    },
    {
      "name": "bugfix-*",
      "prerelease": true
    },
    {
      "name": "hotfix-*",
      "prerelease": true
    },
    {
      "name": "build-*",
      "prerelease": true
    },
    {
      "name": "chore-*",
      "prerelease": true
    },
    {
      "name": "ci-*",
      "prerelease": true
    },
    {
      "name": "docs-*",
      "prerelease": true
    },
    {
      "name": "perf-*",
      "prerelease": true
    },
    {
      "name": "refactor-*",
      "prerelease": true
    },
    {
      "name": "revert-*",
      "prerelease": true
    },
    {
      "name": "style-*",
      "prerelease": true
    },
    {
      "name": "test-*",
      "prerelease": true
    },
    {
      "name": "sample-*",
      "prerelease": true
    },
    {
      "name": "security-*",
      "prerelease": true
    },
    {
      "name": "config-*",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      { "changelogFile": "CHANGELOG.md" }
    ],
    [
      "@semantic-release/git",
      { "assets": [ "CHANGELOG.md" ] }
    ],
    "@semantic-release/github"
  ],
  "repositoryUrl": "https://github.com/TechnologyEnhancedLearning/GitPageBlazorWASM"
}